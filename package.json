{
  "name": "pruebatecnica",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prerun": "watchman watch-del-all",
    "clean:node": "rm -rf node_modules && rm -rf yarn.lock && rm -rf package-lock.json && yarn cache clean && yarn install",
    "clean:android": "yarn clean:node && cd android && ./gradlew clean && cd ..",
    "clean:ios": "yarn clean:node &&  cd ios && rm -rf Pods  && rm -rf Podfile.lock  && rm -rf tmp.xcconfig && rm -rf ~/Library/Caches/CocoaPods && rm -rf ~/Library/Developer/Xcode/DerivedData/* && cd .. && yarn pod:reset",
    "pod:reset": "cd ios && pod deintegrate && pod setup && pod install && cd ..",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "prepare": "husky install",
    "format": "npx prettier --write .",
    "format:all": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "start": "react-native start",
    "test": "jest"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-native-masked-view/masked-view": "^0.3.2",
    "@react-navigation/bottom-tabs": "^7.3.14",
    "@react-navigation/devtools": "^7.0.30",
    "@react-navigation/drawer": "^7.4.1",
    "@react-navigation/elements": "^2.4.3",
    "@react-navigation/native": "^7.1.10",
    "@react-navigation/native-stack": "^7.3.14",
    "@react-navigation/stack": "^7.3.3",
    "axios": "^1.9.0",
    "moment": "^2.30.1",
    "react": "19.0.0",
    "react-native": "0.78.2",
    "react-native-config": "^1.5.5",
    "react-native-crypto-js": "^1.0.0",
    "react-native-error-boundary": "^2.0.0",
    "react-native-gesture-handler": "^2.25.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-mmkv": "^3.2.0",
    "react-native-pager-view": "^6.8.1",
    "react-native-reanimated": "3.17.2",
    "react-native-safe-area-context": "^5.4.1",
    "react-native-screens": "^4.11.1",
    "react-native-svg": "^15.12.0",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@babel/runtime": "^7.27.4",
    "@jest/globals": "^29.7.0",
    "@react-native-community/cli": "15.0.1",
    "@react-native-community/cli-platform-android": "15.0.1",
    "@react-native-community/cli-platform-ios": "15.0.1",
    "@react-native/babel-preset": "0.78.2",
    "@react-native/eslint-config": "0.78.2",
    "@react-native/metro-config": "0.78.2",
    "@react-native/typescript-config": "0.78.2",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "^13.2.0",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.0.0",
    "@types/react-native-crypto-js": "^1.0.3",
    "@types/react-test-renderer": "^19.0.0",
    "babel-jest": "^29.7.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^9.28.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "3.5.3",
    "react-native-svg-transformer": "^1.5.1",
    "react-test-renderer": "19.0.0",
    "ts-jest": "^29.3.4",
    "typescript": "5.8.3"
  },
  "engines": {
    "node": ">=18"
  }
}
